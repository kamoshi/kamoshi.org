{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "allowJs": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "baseUrl": "src",
    "paths": {
      "@assets/*": ["assets/*"],
      "@layouts/*": ["layouts/*"],
      "@components/*": ["components/*"],
      "@utils/*": ["utils/*"]
    }
  }
}
